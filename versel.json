{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    },
    {
      "src": "fast_phish_model.joblib",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.py"
    }
  ],
  "functions": {
    "index.py": {
      "memory": 3008,
      "maxDuration": 10
    }
  }
}
